<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qurui.fund_manage_sys.dao.AdminDao">
    <select id="checkAdminLogin" parameterType="AdminLogin" resultType="AdminLogin">
        select *
        from admin_login
        where admin_name = #{admin_name}
          and admin_pwd = #{admin_pwd}
    </select>
</mapper>